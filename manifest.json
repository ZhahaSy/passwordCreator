{
    "name": "PasswordCreator",
    "version": "0.1",
    "manifest_version": 3,
    "description": "密码生成&&存储工具",
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    },
    "action": {
      "default_icon": "icons/icon.png",
      "default_title": "PasswordCreator",
      "default_popup": "popup/index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    
    "permissions": ["contextMenus", "tabs"]
  }
  